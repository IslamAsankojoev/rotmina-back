{
  "kind": "collectionType",
  "collectionName": "variants",
  "info": {
    "singularName": "variant",
    "pluralName": "variants",
    "displayName": "Variant"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "variants"
    },
    "sku": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "stock": {
      "type": "integer"
    },
    "color": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::color.color"
    },
    "size": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::size.size"
    },
    "is_active": {
      "type": "boolean"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
